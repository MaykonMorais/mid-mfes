{
  "name": "header",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "set NEXT_PRIVATE_LOCAL_WEBPACK=true && next dev -p 3005",
    "build": "set NEXT_PRIVATE_LOCAL_WEBPACK=true && next build",
    "start": "set NEXT_PRIVATE_LOCAL_WEBPACK=true && set NODE_ENV=production && next start -p 3001",
    "deploy": "bash deploy.bash",
    "lint": "next lint",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --ci"
  },
  "dependencies": {
    "@emotion/styled": "11.11.0",
    "@module-federation/nextjs-mf": "8.3.14",
    "next": "^14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "webpack": "5.90.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.0.25",
    "@types/testing-library__jest-dom": "5.14.6",
    "esbuild": "0.15.16",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "eslint-config-prettier": "8.5.0",
    "jest": "29.5.0",
    "jest-sonar": "0.2.16",
    "jest-environment-jsdom": "29.5.0",
    "standard-version": "9.5.0",
    "typescript": "4.8.4"
  }
}
